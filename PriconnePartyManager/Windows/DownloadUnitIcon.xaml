<Window x:Class="PriconnePartyManager.Windows.DownloadUnitIcon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:PriconnePartyManager.Windows"
        mc:Ignorable="d"
        Title="アイコンダウンロード" Height="100" Width="500"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="10,10,10,10" VerticalAlignment="Center">
            <TextBlock Text="ダウンロード中…"/>
            <ProgressBar Maximum="{Binding DownloadNum.Value}" Value="{Binding DownloadCompleteNum.Value}" Height="20"/>
        </StackPanel>
    </Grid>
</Window>
